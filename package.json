{
  "name": "elecart-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx babel src --out-dir dist",
    "seed": "npx mongo-seeding-cli -u mongodb://127.0.0.1:27017/elecart --drop-database ./seeds",
    "start": "node dist/index.js",
    "dev": "npx nodemon --legacy-watch src/index.js",
    "prettier": "npx prettier --config ./.prettierrc --write \"**/*.js\"",
    "pretest": "npx eslint --ignore-path .gitignore .",
    "postinstall": "rm -rf dist && yarn run build",
    "lint": "yarn prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
    "release": "release-it patch --no-npm.publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElecartShop/elecart-api.git"
  },
  "author": "Benjamin J. Young",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ElecartShop/elecart-api/issues"
  },
  "homepage": "https://github.com/ElecartShop/elecart-api#readme",
  "dependencies": {
    "apollo-engine": "^1.1.2",
    "apollo-server-express": "^2.12.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "graphql-compose": "^7.14.4",
    "graphql-compose-connection": "^6.0.4",
    "graphql-compose-mongoose": "^7.3.5",
    "graphql-middleware": "^4.0.2",
    "graphql-tools": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.11",
    "mongoose-bcrypt": "^1.6.0",
    "mongoose-timestamp": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "fs-extra": "^9.0.0",
    "mongo-seeding": "^3.4.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  }
}
